{
  "name": "Database",
  "dockerFile": "Dockerfile",
  "context": "../..",
  "runArgs": ["--env-file","database/.env", "-v", "${localWorkspaceFolder}/database:/workspace/database"],
  "workspaceFolder": "/workspace/database",
  "postCreateCommand": "/bin/bash /workspaces/spring2025-team05/scripts/devcontainer/database/startup.sh",
  "forwardPorts": [
    3306
  ],
  "portsAttributes": {
    "3306": {
      "label": "Database",
      "onAutoForward": "notify"
    }
  }
}
